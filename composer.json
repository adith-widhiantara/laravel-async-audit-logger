{
    "name": "adithwidhiantara/laravel-async-audit-logger",
    "description": "High performance async audit logger",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Adithwidhiantara\\Audit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Adith Widhiantara",
            "email": "adityaswidhiantara@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adithwidhiantara\\Audit\\AuditingServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "orchestra/testbench": "^10.8"
    }
}
